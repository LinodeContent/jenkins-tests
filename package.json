  "dependencies": {
    "express": "^4.16.2"
    "forever": "^0.15.3",
    "mocha": "^4.0.1",
    "mocha-junit-reporter": "^1.15.0",
    "should": "^13.1.3",
    "supertest": "^3.0.0"
  }
